{
  "version": "v1",
  "states": [
    "visible+enabled",
    "visible+disabled(reason)",
    "hidden"
  ],
  "rules": {
    "stageGatedDefaultState": "visible+disabled(reason=\"STAGE_GATED\")",
    "minimalProfileCanHideNonCore": true
  },
  "minimalProfileHiddenAllowlist": [
    "cmd.project.importMarkdownV1",
    "cmd.project.exportMarkdownV1",
    "cmd.project.flowOpenV1",
    "cmd.project.flowSaveV1"
  ],
  "coreSafetyCommandAllowlist": [
    "cmd.project.open",
    "cmd.project.save",
    "cmd.project.saveAs",
    "cmd.project.export.docxMin",
    "cmd.project.new",
    "cmd.project.close",
    "cmd.project.recovery.open",
    "cmd.project.recovery.restore"
  ]
}
